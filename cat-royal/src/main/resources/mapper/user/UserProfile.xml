<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenqinhao.cat.royal.dao.user.UserProfileDao">
    <select id="getById" resultType="UserProfile">
        SELECT
          *
        FROM
          user_profile
        WHERE
          id = #{id}
    </select>
</mapper>